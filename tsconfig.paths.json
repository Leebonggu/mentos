{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@pages/*": ["src/pages/*"],
      "@components/*": ["src/components/*"],
      "@styles/*": ["src/styles/*"],
      "@types/*": ["src/types/*"],
      "@apis/*": ["src/apis/*"],
      "@libs/*": ["src/libs/*"],
      "@hooks/*": ["src/hooks/*"],
      "@constants/*": ["src/constants/*"],
      "@modules/*": ["src/modules/*"]
    }
  }
}
